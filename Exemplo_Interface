interface Animal {
	void comer();
	void dormir();
}

class Cachorro implements Animal {
	public void comer() {
		System.out.println("O cachorro está comendo");
	}
	
	public void dormir() {
		System.out.println("O cachorro está dormindo");
	}
}

public class Main {
	public static void main(String[] args) {
		
		Cachorro meuCachorro = new Cachorro();
		
		meuCachorro.comer();
		meuCachorro.dormir();
	}
}
